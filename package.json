  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@nextui-org/react": "^2.4.6",
    "@nextui-org/system": "^2.2.5",
    "@nextui-org/table": "^2.0.39",
    "@nextui-org/theme": "^2.2.9",
    "@prisma/client": "^5.18.0",
    "@radix-ui/react-label": "^2.1.0",
    "@tabler/icons-react": "^3.12.0",
    "axios": "^1.7.5",
    "bootstrap-icons": "^1.11.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.3.30",
    "lucide-react": "^0.435.0",
    "next": "14.2.6",
    "next-auth": "^4.24.7",
    "prisma": "^5.18.0",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.6",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "style": {
      "type": "string",
      "enum": [
        "default",
        "new-york"
      ]
    },
    "tailwind": {
      "type": "object",
      "properties": {
        "config": {
          "type": "string"
        },
        "css": {
          "type": "string"
        },
        "baseColor": {
          "type": "string"
        },
        "cssVariables": {
          "type": "boolean"
        },
        "prefix": {
          "type": "string"
        }
      },
      "required": [
        "config",
        "css",
        "baseColor",
        "cssVariables"
      ]
    },
    "rsc": {
      "type": "boolean"
    },
    "tsx": {
      "type": "boolean"
    },
    "aliases": {
      "type": "object",
      "properties": {
        "utils": {
          "type": "string"
        },
        "components": {
          "type": "string"
        },
        "ui": {
          "type": "string"
        }
      },
      "required": [
        "utils",
        "components"
      ]
    }
  },
  "required": [
    "style",
    "tailwind",
    "rsc",
    "aliases"
  ]
}
